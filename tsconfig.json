{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "jsx": "React",
    "lib": ["DOM", "ES2015", "ES6", "DOM.Iterable", "ScriptHost"],
    "module": "ES6",
    "moduleResolution": "Node",
    "noEmit": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES5"
  }
}